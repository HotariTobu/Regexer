<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>icon.ico</ApplicationIcon>
    <AssemblyVersion>1.3</AssemblyVersion>
    <FileVersion>1.3</FileVersion>
    <Copyright>Copyright © 2021 hotari13port@gmail.com</Copyright>
    <Description>リアルタイムに正規表現で検索・置換します。
ファイル処理にも対応しています。</Description>
    <Company></Company>
    <Authors>Hotari</Authors>
    <SignAssembly>True</SignAssembly>
    <AssemblyOriginatorKeyFile>D:\Visual Studio\VSProjects\Cs\署名\hotari.pfx</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <Import Project="..\..\SharedWPF\SharedWPF\SharedWPF.projitems" Label="Shared" />

  <Import Project="..\..\Encodable\Encodable\Encodable.projitems" Label="Shared" />

  <ItemGroup>
    <Content Include="icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Page Update="Resources\String\ja-jp.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
    </Page>
  </ItemGroup>

</Project>
